{
    "openapi": "3.0.2",
    "info": {
        "title": "EVM JSON-RPC API: trace_block",
        "version": "v1.0.0"
    },
    "servers": [
        {
            "url": "https://{network}.s.chainbase.online/v1",
            "variables": {
                "network": {
                    "enum": [
                        "ethereum-mainnet",
                        "polygon-mainnet",
                        "bsc-mainnet"
                    ],
                    "default": "ethereum-mainnet"
                }
            }
        }
    ],
    "paths": {
        "/{api-key}": {
            "post": {
                "summary": "trace_block",
                "description": "Returns traces created at given block.\n\n**Parameters**\n\nTAG - Integer of a block number, or the string 'earliest', 'latest' or 'pending'.\n\n**Returns**\n\nObject - An object of type AdhocTraceArray.",
                "operationId": "evm-trace-block",
                "parameters": [
                    {
                        "required": true,
                        "schema": {
                            "title": "Api-Key",
                            "type": "string",
                            "example": "demo"
                        },
                        "name": "api-key",
                        "in": "path"
                    }
                ],
                "requestBody": {
                    "content": {
                        "application/json": {
                            "schema": {
                                "$ref": "#/components/schemas/RequestExample"
                            }
                        }
                    },
                    "required": true
                },
                "responses": {
                    "200": {
                        "description": "Successful Response",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/ResponseParityTraces"
                                }
                            }
                        }
                    }
                }
            }
        }
    },
    "components": {
        "schemas": {
            "ParityTrace": {
                "title": "ParityTrace",
                "required": [
                    "action",
                    "blockHash",
                    "blockNumber",
                    "result",
                    "subtraces",
                    "traceAddress",
                    "transactionHash",
                    "transactionPosition",
                    "type"
                ],
                "type": "object",
                "properties": {
                    "action": {
                        "$ref": "#/components/schemas/ParityTraceAction"
                    },
                    "blockHash": {
                        "title": "Blockhash",
                        "type": "string"
                    },
                    "blockNumber": {
                        "title": "Blocknumber",
                        "type": "string"
                    },
                    "result": {
                        "$ref": "#/components/schemas/ParityTraceResult"
                    },
                    "subtraces": {
                        "title": "Subtraces",
                        "type": "integer"
                    },
                    "traceAddress": {
                        "title": "Traceaddress",
                        "type": "array",
                        "items": {
                            "type": "integer"
                        }
                    },
                    "transactionHash": {
                        "title": "Transactionhash",
                        "type": "string"
                    },
                    "transactionPosition": {
                        "title": "Transactionposition",
                        "type": "integer"
                    },
                    "type": {
                        "title": "Type",
                        "type": "string"
                    }
                }
            },
            "ParityTraceAction": {
                "title": "ParityTraceAction",
                "required": [
                    "from",
                    "callType",
                    "gas",
                    "input",
                    "to",
                    "value"
                ],
                "type": "object",
                "properties": {
                    "from": {
                        "title": "From",
                        "type": "string"
                    },
                    "callType": {
                        "title": "Calltype",
                        "type": "string"
                    },
                    "gas": {
                        "title": "Gas",
                        "type": "string"
                    },
                    "input": {
                        "title": "Input",
                        "type": "string"
                    },
                    "to": {
                        "title": "To",
                        "type": "string"
                    },
                    "value": {
                        "title": "Value",
                        "type": "string"
                    }
                }
            },
            "ParityTraceResult": {
                "title": "ParityTraceResult",
                "required": [
                    "gasUsed",
                    "output"
                ],
                "type": "object",
                "properties": {
                    "gasUsed": {
                        "title": "Gasused",
                        "type": "string"
                    },
                    "output": {
                        "title": "Output",
                        "type": "string"
                    }
                }
            },
            "RequestExample": {
                "title": "RequestExample",
                "required": [
                    "id",
                    "jsonrpc",
                    "method",
                    "params"
                ],
                "type": "object",
                "properties": {
                    "id": {
                        "title": "Id",
                        "type": "integer",
                        "default": 1,
                        "example": 1
                    },
                    "jsonrpc": {
                        "title": "Jsonrpc",
                        "type": "string",
                        "default": "2.0",
                        "example": "2.0"
                    },
                    "method": {
                        "title": "Method",
                        "type": "string",
                        "default": "trace_block"
                    },
                    "params": {
                        "title": "Params",
                        "type": "array",
                        "items": {},
                        "default": [
                            "latest"
                        ]
                    }
                },
                "example": {
                    "id": 1,
                    "jsonrpc": "2.0",
                    "method": "trace_block",
                    "params": [
                        "latest"
                    ]
                }
            },
            "ResponseParityTraces": {
                "title": "ResponseParityTraces",
                "required": [
                    "id",
                    "jsonrpc",
                    "result"
                ],
                "type": "object",
                "properties": {
                    "id": {
                        "title": "Id",
                        "type": "integer",
                        "example": 1
                    },
                    "jsonrpc": {
                        "title": "Jsonrpc",
                        "type": "string",
                        "example": "2.0"
                    },
                    "result": {
                        "title": "Result",
                        "type": "array",
                        "items": {
                            "$ref": "#/components/schemas/ParityTrace"
                        }
                    }
                }
            }
        }
    }
}